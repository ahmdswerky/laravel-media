{
    "name": "ahmdswerky/laravel-media",
    "description": "Media package for uploading photos, videos and files to your Laravel app",
    "keywords": [
        "laravel",
        "media",
        "upload",
        "photos",
        "videos",
        "files"
    ],
    "version": "v1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Swerky",
            "email": "ahmdswerky@gmail.com",
            "homepage": "https://swerky.me",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/ahmdswerky/laravel-media",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AhmdSwerky\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmdSwerky\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AhmdSwerky\\Media\\MediaBaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
